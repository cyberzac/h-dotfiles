â€º ]133;B[K[132C[01;36m[00m[132D[?2004hlless  l  ccat docker      initstack[1m [0m[0m [?2004l
]133;C;#!/bin/bash -x
stack=$(hostname -s)
conf=${stack}_conf

docker volume create $conf 
tar Ccf conf - . | docker run --rm -v $conf:/conf -i alpine tar -C /conf -x -f -

docker run --rm -v ${stack}_data:/data alpine sh -c 'for service in account payment stroem; do mkdir -p /data/$service; done'

for volume in wallets data; do
    docker run --rm -v ${stack}_$volume:/$volume alpine chown -R daemon:daemon /$volume
done

docker stack deploy --compose-file strawpay-compose.yaml $stack
